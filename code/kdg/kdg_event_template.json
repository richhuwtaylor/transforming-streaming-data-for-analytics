        ["amazon prime", "google play", "apple itunes", "vudo", "fandango now", "microsoft", "youtube"]
    )}}",
    "platform":"{{random.arrayElement(
        ["ios", "android", "xbox", "playstation", "smart tv", "other"]
    )}}",
    "state": "{{address.state}}"
}